<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="50dp"
    android:background="#f5f5f5"
    tools:context="com.example.android.restukuatno_1202150015_modul2.Splashscreen">

    <ImageView
        android:id="@+id/logopringsewu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="102dp"
        android:contentDescription="Pringsewu Logo"
        android:src="@drawable/logopringsewu" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="@android:style/Widget.Holo.ProgressBar.Large"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_below="@id/logopringsewu"
        android:layout_marginTop="30dp" />

    <TextView
        android:id="@+id/presento"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/progressBar"
        android:text="present to"
        android:textAlignment="center"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        android:textColor="#999" />

    <ImageView
    android:id="@+id/logoprings"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:layout_marginTop="2dp"
    android:layout_below="@+id/presento"
    android:contentDescription="Pringsewu Logo"
    android:src="@drawable/logopringsewu" />

</RelativeLayout>
